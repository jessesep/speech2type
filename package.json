{
  "name": "speech2type-fork",
  "version": "0.2.0",
  "description": "A minimal CLI tool that gives you quick voice typing in every Mac app. Fork with voice commands.",
  "author": "jessesep",
  "license": "MIT",
  "homepage": "https://github.com/jessesep/speech2type-fork",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessesep/speech2type-fork.git"
  },
  "keywords": [
    "speech-to-text",
    "voice-typing",
    "dictation",
    "macos",
    "cli"
  ],
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "speech2type": "bin/speech2type.js",
    "s2t": "bin/speech2type.js"
  },
  "files": [
    "bin/",
    "src/",
    "swift/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "os": [
    "darwin"
  ],
  "cpu": [
    "arm64"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/jessesep/speech2type-fork/issues"
  },
  "scripts": {
    "start": "./bin/speech2type.js",
    "build:swift": "mkdir -p bin && swiftc -O -o bin/hotkey-manager swift/hotkey-manager.swift && swiftc -O -o bin/permission-checker swift/permission-checker.swift && swiftc -O -o bin/mic-recorder swift/mic-recorder.swift",
    "build": "npm run build:swift",
    "dev": "npm run build && ./bin/speech2type.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "push": "git add . && git commit -m 'ship!' && git push",
    "release": "npm version patch && npm publish"
  },
  "preferGlobal": true,
  "dependencies": {
    "@deepgram/sdk": "^3.6.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "conf": "^10.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2"
  }
}
